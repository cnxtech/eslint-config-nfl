{
  "name": "eslint-config-nfl",
  "version": "12.0.0",
  "description": "NFL's ESLint config",
  "main": "index.js",
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git-cz",
    "lint": "eslint .",
    "tests-only": "babel-tape-runner ./test/test-*.js",
    "test": "parallelshell 'npm run --silent lint' 'npm run --silent tests-only'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nfl/eslint-config-nfl"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
    "nfl"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nfl/eslint-config-nfl/issues"
  },
  "homepage": "https://github.com/nfl/eslint-config-nfl",
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "babel-tape-runner": "^1.3.1",
    "conventional-changelog-cli": "^1.2.0",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^4.4.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-react": "^7.1.0",
    "parallelshell": "^2.0.0",
    "react": "^15.4.1",
    "tape": "^4.6.3"
  },
  "peerDependencies": {
    "babel-eslint": ">=6.x",
    "eslint": ">=4.x.x",
    "eslint-plugin-import": ">=2.x.x",
    "eslint-plugin-react": ">=7.x"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
